<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="secuencia_presupuesto_pelicula" model="ir.sequence">
    <!-- nombre de la secuencia -->
        <field name="name">Secuencia para presupuestos de peliculas</field>
        <!-- sin hueco -->
        <field name="implementation">no_gap</field>
        <!-- codigo de la secuencia, es estring -->
        <field name="code">secuencia.presupuesto.peliculas</field>
        <!-- va al inicio de la secuencia -->
        <field name="prefix">PP-</field>
        <!-- lo que va al final de la secuencia -->
        <field name="suffix">-%(year)s</field>
        <!-- tamaÃ±o de la secuencia, cantidad de numeros de la secuencia de numeros, los llena con 0 -->
        <field name="padding">3</field>
        <!-- paso: aumenta de 1 en 1 -->
        <field name="number_increment">1</field>
    </record>

</odoo>