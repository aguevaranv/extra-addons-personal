<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="secuencia_micro_ticket" model="ir.sequence">
    <!-- nombre de la secuencia -->
        <field name="name">Secuencia para ticket del sistema micro</field>
        <!-- codigo de la secuencia, es estring -->
        <field name="code">secuencia.micro.ticket</field>
        <!-- sin hueco -->
        <field name='active'>TRUE</field>
        <field name="implementation">no_gap</field>
        <!-- va al inicio de la secuencia -->
        <field name="prefix">001-001-</field>
        <!-- lo que va al final de la secuencia -->
        <field name="suffix"></field>
        <!-- tamaño de la secuencia, cantidad de numeros de la secuencia de numeros, los llena con 0 -->
        <field name="padding">9</field>
        <!-- paso: aumenta de 1 en 1 -->
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="secuencia_micro_movimiento" model="ir.sequence">
        <!-- nombre de la secuencia -->
            <field name="name">Secuencia para movimiento del sistema micro</field>
            <!-- codigo de la secuencia, es estring -->
            <field name="code">secuencia.micro.movimiento</field>
            <!-- sin hueco -->
            <field name='active'>TRUE</field>
            <field name="implementation">no_gap</field>
            <!-- va al inicio de la secuencia -->
            <field name="prefix"></field>
            <!-- lo que va al final de la secuencia -->
            <field name="suffix"></field>
            <!-- tamaño de la secuencia, cantidad de numeros de la secuencia de numeros, los llena con 0 -->
            <field name="padding">9</field>
            <!-- paso: aumenta de 1 en 1 -->
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

</odoo>